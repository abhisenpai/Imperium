<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.imperium">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/launcherapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SPLASH_SCREEN"
            android:theme="@style/NoActionTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CHANGES" />
        <activity android:name=".MainActivity" />
        <activity android:name=".DeviceActivity" />
        <activity android:name=".CurrentActivity" />
        <activity android:name=".GetStartedActivity" />
        <activity android:name=".PowerActivity" />
        <activity android:name=".GetStartedActivity2"></activity>
    </application>
</manifest>
